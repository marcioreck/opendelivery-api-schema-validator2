{
    "name": "opendelivery/laravel-validator",
    "description": "OpenDelivery API Schema Validator package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MÃ¡rcio Reck",
            "email": "contact@fazmercado.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "justinrainbow/json-schema": "^6.0",
        "symfony/yaml": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "OpenDelivery\\LaravelValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenDelivery\\LaravelValidator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenDelivery\\LaravelValidator\\OpenDeliveryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
